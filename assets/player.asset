GameObject:
  tag: Player
  components:
  - $MeshRenderer
  - $Transform
  - $PlayerController
  - $SphereCollider
  - $RigidBody
MeshRenderer:
  mesh: hero.glb$hero_mesh
  material: playerMaterial.asset
Transform:
  pos: [0,0.5,0]
  euler: [0, 60, 0]
  scale: [0.7, 0.7, 0.7]
PlayerController:
  speed: 4.2
  jumpPushImpulse: 500
  jumpPushRadius: 10
  bulletSpawnOffset: 0.5
  shootingLightPrefab: ShootingLight.asset
  shootingLightOffset: [0, 1, 3]
  bulletSpeed: 20
SphereCollider:
  radius: 1
  center: [0, 0.5, 0]
RigidBody:
  isStatic: false
  isKinematic: false
  restitution: 0.0
  friction: 0.0
  mass: 5000
  layer: player
PointLight:
  color: 0xffffffff
  radius: 10
  innerRadius: 0.0
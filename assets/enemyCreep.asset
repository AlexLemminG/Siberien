GameObject:
  tag: "Creep"
  components:
  - $MeshRenderer
  - $Transform
  - $EnemyCreepController
  - $SphereCollider
  - $RigidBody
  - $Health
  - $Animator
MeshRenderer:
  mesh: meshes/zombie.glb$Zombie
  material: enemyMaterial.asset
Transform:
  pos: [5,0.5,0]
  euler: [0, 60, 0]
  scale: [0.6, 0.6, 0.6]
EnemyCreepController:
  targetTag: Player
  speed: 6.0
  angularSpeed: 10.0
  acc: 5
  angularAcc: 3.4
  velocityToAnimatorSpeed: 3.0
  walkAnimation: meshes/zombie.glb$Walk
  rollAnimation: meshes/zombie.glb$Roll
  attackAnimation: meshes/zombie.glb$Attack
SphereCollider:
  radius: 0.8
  center: [0, 0.15, 0]
RigidBody:
  isStatic: false
  friction: 0.3
  restitution: 0
  mass: 50
  layer: enemy
Health:
  amount: 1
Animator:
  speed: 10
  defaultAnimation: meshes/zombie.glb$Walk
  